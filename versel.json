{
  "builds": [
    {"src": "build_files.sh", "use": "@vercel/static-build", "config": {"distDir": "staticfiles_build"}},
    {"src": "manage.py", "use": "@vercel/python"}
  ],
  "routes": [
    {"src": "/static/(.*)", "dest": "staticfiles_build/static/$1"},
    {"src": "/(.*)", "dest": "manage.py"}
  ]
}